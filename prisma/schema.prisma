// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Membro {
  id           String   @id @default(cuid())
  nome         String
  grau         String
  status       String
  dataCadastro DateTime @default(now())
  
  @@map("membros")
}